<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
<extension point="org.eclipse.ui.commands">
    <command
          id="net.sf.jmoney.commands.exportPortfolio"
          name="Export Portfolio">
    </command>
</extension>

<extension point="org.eclipse.ui.menus">
     <menuContribution locationURI="menu:navigate?after=openEditors">
        <command commandId="net.sf.jmoney.commands.openExcel"
                 label="Export into Spreadsheet..."
                 tooltip="Export Portfolio to Spreadsheet">
        </command>
     </menuContribution>
</extension>
 
	<extension point="org.eclipse.ui.exportWizards">
		<wizard
        class="net.sf.jmoney.portfolioanalysis.PortfolioExportWizard"
        id="net.sf.jmoney.portfolioanalysis.exportwizard"
        name="Export Stock Portfolio to Spreadsheet">
			<description>
				Export Stock Portfolio to Spreadsheet. 
			</description>
		</wizard>
	</extension>
 
</plugin>
